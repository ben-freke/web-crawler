{
  "name": "simple-web-crawler",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "license": "",
  "scripts": {
    "webcrawler": "ts-node src/main.ts",
    "build": "esbuild src/main.ts --platform=node --main-fields=module,main --bundle --target=node22 --outdir=./build --minify",
    "eslint:check": "eslint --ext mjs,jsx,js,tsx,ts src",
    "eslint:fix": "npm run eslint:check -- --fix --cache --format codeframe",
    "check-all": "npm run eslint:check",
    "fix-all": "npm run eslint:fix",
    "lint": "npm run fix-all && npm run lint-ci",
    "lint-ci": "tsc --noEmit && npm run check-all",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@smithy/types": "^4.2.0",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^22.x",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "esbuild": "^0.25.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.5.4",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "engines": {
    "node": "=22",
    "npm": "=10"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "boxen": "^8.0.1",
